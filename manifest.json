{
  "name": "Weather Forecast",
  "description": "Weather Forecast",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "geolocation"],
  "action": {
    "default_icon": {
      "128": "images/assets/128.png",
      "32": "images/assets/32.png",
      "24": "images/assets/24.png",
      "16": "images/assets/16.png"
    },
    "default_title": "Weather Forecast",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/assets/128.png",
    "32": "images/assets/32.png",
    "24": "images/assets/24.png",
    "16": "images/assets/16.png"
  }
}
